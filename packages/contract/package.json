    "directory": "examples/typescript-vite-package"
  },
  "keywords": [
    "dinwwwh"
  ],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/src/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
      },
      "./__internal__/*": {
        "types": "./dist/src/*.d.ts"
      }
    }
  },
  "exports": {
    ".": "./src/index.ts",
    "./__internal__/*": "./src/*.ts"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "type:check": "tsc -b",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "devDependencies": {
    "@dinwwwh/tsconfig": "^0.0.6",
    "@types/node": "^22.5.5",
    "typescript": "^5.6.2",
    "vite": "^5.4.6",
    "vite-plugin-dts": "^4.2.1",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.1"
  }
}
